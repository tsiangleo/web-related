<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:services="http://www.alibaba.com/schema/services"
             xmlns:engines="http://www.alibaba.com/schema/services/template/engines"
             xsi:schemaLocation="
                 http://www.alibaba.com/schema/services http://localhost:8080/schema/services.xsd
                 http://www.alibaba.com/schema/services/template/engines http://localhost:8080/schema/services-template-engines.xsd
                 http://www.springframework.org/schema/beans http://localhost:8080/schema/www.springframework.org/schema/beans/spring-beans.xsd
             ">

    <services:template>
        <engines:template-engine id="myEngine" class="com.alibaba.citrus.service.template.impl.SimpleEngine" />

        <engines:template-engine id="myEngine2" class="com.alibaba.citrus.service.template.impl.SimpleEngine" />

        <template-mapping extension="test" engine="myEngine1" />
        <template-mapping extension=" .test " engine="myEngine2" />
    </services:template>

</beans:beans>